{
  "name": "agendash",
  "version": "1.0.0",
  "description": "Agenda Dashboard",
  "main": "app.js",
  "bin": "bin/agendash-standalone.js",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "test": "run-p lint ava",
    "lint": "xo",
    "ava": "ava -v -c 1"
  },
  "keywords": [
    "agenda"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenda/agendash.git"
  },
  "bugs": {
    "url": "https://github.com/agenda/agendash/issues"
  },
  "homepage": "https://github.com/agenda/agendash#readme",
  "license": "MIT",
  "dependencies": {
    "@hapi/hapi": "^20.0",
    "@hapi/inert": "^6.0",
    "agenda": "^4.0",
    "async": "^3.2",
    "body-parser": "^1.19",
    "commander": "^7.0",
    "express": "^4.17",
    "semver": "^7.3"
  },
  "devDependencies": {
    "ava": "^3.15",
    "npm-run-all": "^4.1",
    "supertest": "^6.1",
    "xo": "^0.37"
  },
  "xo": {
    "space": 2,
    "rules": {
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "max-params": [
        "error",
        5
      ],
      "max-nested-callbacks": [
        "error",
        5
      ]
    },
    "envs": [
      "node"
    ],
    "ignores": [
      "public/**/*"
    ]
  }
}
